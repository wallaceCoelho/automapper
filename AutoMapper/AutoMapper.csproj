<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net9.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageId>AutoMapper.WallaceCoelho</PackageId>
        <Version>1.0.0</Version>
        <Authors>Wallace Coelho</Authors>
        <Company>Coelho.Devs</Company>
        <Description>AutoMapper manual de alta performance com cache e com suporte a objetos complexos.</Description>
        <PackageTags>mapper automapper performance csharp</PackageTags>
        <RepositoryUrl>https://github.com/wallaceCoelho/automapper</RepositoryUrl>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="10.0.0-preview.2.25163.2" />
    </ItemGroup>

</Project>
